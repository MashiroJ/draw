<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mashiro.mapper.DrawRecordMapper">

    <update id="updateDrawLikeCount">
        UPDATE draw_record
        SET like_count = #{newLikeCount}
        WHERE id = #{drawId}
    </update>
</mapper>